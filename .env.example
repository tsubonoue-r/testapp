# Lark (Feishu) API Credentials
# Get these from: https://open.feishu.cn/app
LARK_APP_ID=your_app_id_here
LARK_APP_SECRET=your_app_secret_here

# Lark Base Configuration
# App Token: Found in the Base URL (e.g., https://xxx.feishu.cn/base/XXXX)
# Table ID: Found in the table settings or URL
LARK_BASE_APP_TOKEN=your_base_app_token_here
LARK_BASE_TABLE_ID=your_table_id_here

# Lark Base Field Mapping (Field IDs from your table)
# These are the field IDs in your Lark Base table
LARK_FIELD_PROJECT_NAME=project_name
LARK_FIELD_DESCRIPTION=description
LARK_FIELD_LOCATION=location
LARK_FIELD_START_DATE=start_date
LARK_FIELD_END_DATE=end_date
LARK_FIELD_STATUS=status
LARK_FIELD_PDF=photo_ledger_pdf

# GitHub Token (for Miyabi agents)
GITHUB_TOKEN=your_github_token_here

# Anthropic API Key (for Miyabi agents)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
